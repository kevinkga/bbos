{
  "name": "@bbos/backend",
  "version": "1.0.0",
  "description": "BBOS Backend - Node.js server with real-time build management",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "docker:build": "docker build -t bbos-backend .",
    "docker:run": "docker run -p 3001:3001 bbos-backend",
    "db:migrate": "tsx src/scripts/migrate.ts",
    "db:seed": "tsx src/scripts/seed.ts",
    "generate:types": "tsx src/scripts/generate-types.ts"
  },
  "dependencies": {
    "express": "^4.19.2",
    "socket.io": "^4.7.5",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.2.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-github2": "^0.1.12",
    "passport-discord": "^0.1.4",
    "passport-microsoft": "^1.0.0",
    "express-session": "^1.18.0",
    "connect-redis": "^7.1.1",
    "redis": "^4.6.13",
    "prisma": "^5.12.1",
    "@prisma/client": "^5.12.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "ajv": "^8.13.0",
    "ajv-formats": "^3.0.1",
    "zod": "^3.22.5",
    "winston": "^3.13.0",
    "bull": "^4.12.9",
    "ioredis": "^5.3.2",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1605.0",
    "sharp": "^0.33.3",
    "archiver": "^7.0.1",
    "tar": "^7.0.1",
    "yaml": "^2.4.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "express-validator": "^7.0.1",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "dotenv": "^16.4.5",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-github2": "^1.2.9",
    "@types/express-session": "^1.18.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/uuid": "^9.0.8",
    "@types/multer": "^1.4.11",
    "@types/archiver": "^6.0.2",
    "@types/tar": "^6.1.11",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "typescript": "^5.4.2",
    "tsx": "^4.7.2",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "vitest": "^1.4.0",
    "@vitest/coverage-v8": "^1.4.0",
    "supertest": "^6.3.4",
    "@types/supertest": "^6.0.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
} 